package code{	 	import com.phidgets.*;	import com.phidgets.events.*;		import flash.events.Event;	import flash.events.MouseEvent;	import flash.events.KeyboardEvent;	import flash.display.SimpleButton;	import flash.ui.Keyboard;	import flash.utils.*;		public class RFIDController	{		//*************************						var rfid1:PhidgetRFID = null;		var rfid2:PhidgetRFID = null;		var gameController:GameController = null;										public function RFIDController(gameController:GameController)		{			this.gameController = gameController;			rfid1 = new PhidgetRFID();			rfid2 = new PhidgetRFID();			rfid1.addEventListener(PhidgetEvent.DETACH,	onDetach);			rfid1.addEventListener(PhidgetEvent.ATTACH,	onAttach);			rfid1.addEventListener(PhidgetErrorEvent.ERROR, onError);			rfid1.addEventListener(PhidgetDataEvent.TAG, onTag);			rfid1.addEventListener(PhidgetDataEvent.TAG_LOST, onTagLoss);			rfid2.addEventListener(PhidgetEvent.DETACH,	onDetach);			rfid2.addEventListener(PhidgetEvent.ATTACH,	onAttach);			rfid2.addEventListener(PhidgetErrorEvent.ERROR, onError);			rfid2.addEventListener(PhidgetDataEvent.TAG, onTag);			rfid2.addEventListener(PhidgetDataEvent.TAG_LOST, onTagLoss);			rfid1.open("localhost", 5001, null, 4787);														}										function onError(evt:PhidgetErrorEvent):void {			trace(evt);		}				function onAttach(evt:PhidgetEvent):void{						var rfid = evt.Device;			trace(evt + " " + rfid.serialNumber);			rfid.Antenna = true;			rfid.LED = true;					}				function onDetach(evt:PhidgetEvent):void{			trace(evt);		}				function onTag(evt:PhidgetDataEvent):void{			//trace(evt);			trace("Tag: " + getQualifiedClassName(evt.Data));						var rfid = evt.Device;			if(rfid == rfid1) {				gameController.newCard(1, 5);							} else if(rfid = rfid2) {				gameController.newCard(2, 5);			} else {				trace("Error, RFID Reader not found...");			}								}		function onTagLoss(evt:PhidgetDataEvent):void{			trace(evt);		}			}}