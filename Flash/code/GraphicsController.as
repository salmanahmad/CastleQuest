package code{	/*****************************************	 * Game1 :	 * Demonstrates a maze game and user interface.	 * -------------------	 * See 1_maze.fla	 ****************************************/	 	import flash.events.Event;	import flash.events.MouseEvent;	import flash.events.KeyboardEvent;	import flash.display.MovieClip;	import flash.display.SimpleButton;	import flash.ui.Keyboard;		public class CastleQuest extends MovieClip	{		//*************************				var cannonAnimation:CannonLaunch = null;				public function CastleQuest()		{			trace("Here");												// Respond to mouse events			start_button.addEventListener(MouseEvent.CLICK,clickHandler);																	  						// Update screen every frame			addEventListener(Event.ENTER_FRAME,enterFrameHandler);		}				//*************************		// Event Handling:				protected function enterFrameHandler(event:Event):void		{						if(cannonAnimation && cannonAnimation.currentFrame == cannonAnimation.totalFrames) {				cannonAnimation = null;				castle2.gotoAndPlay("destroy");			}		}			protected function clickHandler(event:MouseEvent):void		{			trace("Hi!");			cannonAnimation = new CannonLaunch();			cannonAnimation.x = 50;			cannonAnimation.y = 300;			addChild(cannonAnimation);		}					}}